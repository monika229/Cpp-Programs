// kth smallest element 
#include<iostream>
#include<bits/stdc++.h>
using namespace std;
int main() {
    int t;
    cin>>t;
    for(int i=0;i<t;i++)
    {
        int n,k,a[n];
        cin>>n;
        for(int i=0;i<n;i++)
            cin>>a[i];
        cin>>k;
        sort(a,a+n);
        cout<<a[k-1]<<endl;
    }
	//code
	return 0;
}
